.w_live_cover {
  height: 340rpx;
  border-bottom: 1px solid #d4d4d4;
  box-sizing: border-box;
  padding: 30rpx;
}

.w_live_cover .w_title {
  margin-left: 20rpx;
}

.w_live_cover .w_addcover_img {
  width: 200rpx;
  height: 200rpx;
  border: 1px solid #e6e6e6;
  margin-top: 20rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}

.w_live_cover .w_addcover_img image {
  width: 60rpx;
  height: 60rpx;
}

.w_live_cover .w_cover_img {
  width: 200rpx;
  height: 200rpx;
  margin-top: 20rpx;
  background:yellow;
  display:flex;
  justify-content:flex-end;
}
.w_live_cover .w_cover_img image{
  width:50rpx;
  height:50rpx;
}
.w_pusher_title{
  height:100rpx;
  border-bottom: 1px solid #d4d4d4;
  padding-left:40rpx;
  display:flex;
  align-items:center;
  font-size:30rpx;
}
.w_pusher_title text{
  margin-right:20rpx;
}

.w_start_pusher{
  margin:0 auto;
  width:450rpx;
  height:90rpx;
  background: -webkit-linear-gradient(left,#EA75E8, #695EFD);
  color:#fff;
  display:flex;
  justify-content:center;
  align-items:center;
  border-radius:50rpx;
  margin-top:30rpx;
}
